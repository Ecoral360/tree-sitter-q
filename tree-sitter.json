{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "q",
      "camelcase": "Q",
      "title": "Q",
      "scope": "source.q",
      "file-types": [
        "q"
      ],
      "injection-regex": "^q$",
      "class-name": "TreeSitterQ"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Q programming language",
    "authors": [
      {
        "name": "Mathis",
        "email": "ecomath360@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/Ecoral360/tree-sitter-q"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}